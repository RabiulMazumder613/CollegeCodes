//smaller.cpp
#include <iostream>
using namespace std;

int main() 
{
    int x;
    cout << "Enter the first number: ";
    cin >> x;
    int y;
    cout << "Enter the second number: ";
    cin >> y;
    if (x < y)
    {
        cout<<"The smaller of the two is " << x << endl;
    }
    
    else 
    {
        cout<<"The smaller of the two is " << y << endl;
    }
    return 0;
}
